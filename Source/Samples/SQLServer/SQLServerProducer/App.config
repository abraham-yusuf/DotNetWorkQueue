<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <appSettings>
      <add key="Database" value="Server=V-SQL;Application Name=Samples;Database=TestR;Trusted_Connection=True;max pool size=500" />
